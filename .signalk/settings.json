{
    "courseApi": {
        "apiOnly": false
    },
    "interfaces": {},
    "pipedProviders": [
        {
            "enabled": true,
            "id": "OpenPlotter SDR AIS",
            "pipeElements": [
                {
                    "options": {
                        "logging": false,
                        "subOptions": {
                            "port": "10110",
                            "type": "udp",
                            "validateChecksum": true
                        },
                        "type": "NMEA0183"
                    },
                    "type": "providers\/simple"
                }
            ]
        },
        {
            "enabled": true,
            "id": "gnss",
            "pipeElements": [
                {
                    "options": {
                        "logging": false,
                        "subOptions": {
                            "baudrate": 38400,
                            "device": "\/dev\/ttyOP_gnss",
                            "type": "serial",
                            "validateChecksum": true
                        },
                        "type": "NMEA0183"
                    },
                    "type": "providers\/simple"
                }
            ]
        }
    ],
    "resourcesApi": {
        "defaultProviders": {
            "charts": "resources-provider",
            "notes": "resources-provider",
            "regions": "resources-provider",
            "routes": "resources-provider",
            "waypoints": "resources-provider"
        }
    }
}